{
  "name": "@gasbuddy/meddleware",
  "version": "1.0.0",
  "description": "Configuration-based middleware registration for express.",
  "main": "build/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap --node-arg=-r --node-arg=babel-register test/*.js",
    "cover": "istanbul cover tap -- --node-arg=-r --node-arg=babel-register test/*.js",
    "dryrun": "(TARBALL=`npm pack` ; tar ztvf $TARBALL ; rm -f $TARBALL)",
    "build": "babel src --out-dir build",
    "prepublish": "npm run build"
  },
  "keywords": [
    "express",
    "middleware"
  ],
  "author": "Erik Toth <ertoth@paypal.com>",
  "licenses": [
    {
      "type": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-gasbuddy": "^1.0.0",
    "body-parser": "^1.0.2",
    "cookie-parser": "^1.0.1",
    "express": "^4.6.1",
    "istanbul": "^0.2.7",
    "morgan": "^1.0.0",
    "serve-favicon": "^2.0.1",
    "serve-static": "^1.0.4",
    "shortstop": "^1.0.1",
    "shortstop-handlers": "^1.0.0",
    "shortstop-regex": "0.0.1",
    "supertest": "^0.11.0",
    "tap": "^8.0.1"
  },
  "dependencies": {
    "caller": "0.0.1",
    "debuglog": "0.0.4",
    "core-util-is": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/krakenjs/meddleware.git"
  },
  "bugs": {
    "url": "https://github.com/krakenjs/meddleware/issues"
  },
  "homepage": "https://github.com/krakenjs/meddleware"
}
