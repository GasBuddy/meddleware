{
    "favicon": {
        "enabled": true,
        "priority": 10,
        "module": "static-favicon"
    },

    "static": {
        "enabled": true,
        "priority": 30,
        "module": {
            "name": "serve-static",
            "arguments": [ "path:./.build" ]
        }
    },

    "logger": {
        "enabled": true,
        "priority": 50,
        "module": "morgan"
    },

    "json": {
        "enabled": true,
        "priority": 60,
        "module": {
            "name": "body-parser",
            "method": "json"
        }
    },

    "urlencoded": {
        "enabled": true,
        "priority": 70,
        "module": {
            "name": "body-parser",
            "method": "urlencoded"
        }
    },

    "cookieParser": {
        "enabled": true,
        "priority": 90,
        "module": {
            "name": "cookie-parser",
            "arguments": [ "keyboard cat" ]
        }
    }

}