{
    "favicon": {
        "enabled": true,
        "priority": 10,
        "module": "static-favicon",
        "arguments": null
    },

    "static": {
        "enabled": true,
        "priority": 30,
        "module": "serve-static",
        "arguments": [ "path:./.build" ]
    },

    "logger": {
        "enabled": true,
        "priority": 50,
        "module": "morgan",
        "arguments": null
    },

    "json": {
        "enabled": true,
        "priority": 60,
        "module": "body-parser",
        "factoryMethod": "json"
    },

    "urlencoded": {
        "enabled": true,
        "priority": 70,
        "module": "body-parser",
        "factoryMethod": "urlencoded"
    },

    "cookieParser": {
        "enabled": true,
        "priority": 90,
        "module": "cookie-parser",
        "arguments": [ "keyboard cat" ]
    }

}